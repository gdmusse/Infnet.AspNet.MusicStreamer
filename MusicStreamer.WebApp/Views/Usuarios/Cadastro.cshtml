@model MusicStreamer.ApplicationLayer.Dtos.UsuarioCadastroDto

<h2>Cadastro</h2>

<form asp-action="Cadastro" method="post">
    <div>
        <label>Nome</label>
        <input asp-for="Nome" />
        <span asp-validation-for="Nome"></span>
    </div>
    <div>
        <label>Email</label>
        <input asp-for="Email" />
        <span asp-validation-for="Email"></span>
    </div>
    <div>
        <label>Senha</label>
        <input asp-for="Senha" type="password" />
        <span asp-validation-for="Senha"></span>
    </div>
    <button type="submit">Cadastrar</button>
</form>
